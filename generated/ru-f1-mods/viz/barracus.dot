  "Barracus42" [label="Они жаловались. Но Тон\nслишком много говорит.\nУйди прочь."]
  "Barracus42" -> "BarracusEnd" [label="[Готово]" color="#1A0099" fontcolor="#1A0099"]
  "Barracus43" [style=filled       fillcolor="#FFFFFF"       color="#000000"]
