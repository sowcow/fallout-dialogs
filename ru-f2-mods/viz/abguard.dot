  "Node006" -> "Node001a" [label="Посмотрю, что я смогу\nсделать. А пока позволь\nспросить тебя кое-что\nеще." color="#009599" fontcolor="#009599"]
  "Node006" -> "Node998" [label="Я крутой парень, поэтому\nя собираюсь надрать тебе\nзадницу." color="#997000" fontcolor="#997000"]
