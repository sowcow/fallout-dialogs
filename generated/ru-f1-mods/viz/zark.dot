  "zark35" [style=filled       fillcolor="#FFFFFF"       color="#000000"]
  "zark35" [label="Ты, должно быть, шутишь.\nЭто ты зомби расскажи,\nприятель!"]
  "zark36" [style=filled       fillcolor="#FFFFFF"       color="#000000"]
